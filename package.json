{
  "name": "universities",
  "version": "0.0.3",
  "description": "Comprehensive worldwide universities dataset with TypeScript API, CLI tools, and data processing utilities. Includes web scraping capabilities with respectful rate limiting for enriching university data.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "universities": "dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "world-universities.csv",
    "docs/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "build": "./scripts/build.sh",
    "build:publish": "npm run build:tsc && npm run lint && npm run format:check && npm run test",
    "build:tsc": "rimraf dist && tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts --ext .js",
    "lint:fix": "eslint . --ext .ts --ext .js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "docs": "jsdoc -d docs/jsdoc/ src/**/*.ts",
    "docs:generate": "jsdoc -d docs/jsdoc/ src/**/*.ts",
    "clean": "rimraf dist coverage docs/jsdoc",
    "generate:data": "ts-node ./scripts/generateBaseData.ts",
    "enrich": "ts-node ./scripts/enrich.ts",
    "check": "npm install && npm run build && npm run format && npm run lint && npm run test:coverage",
    "prepare": "npm run build:tsc",
    "prepublishOnly": "npm run build:publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vaporjawn/universities.git"
  },
  "keywords": [
    "university",
    "universities",
    "college",
    "colleges",
    "education",
    "higher-education",
    "academic",
    "institutions",
    "dataset",
    "csv",
    "scraping",
    "typescript",
    "api",
    "cli",
    "data-processing",
    "research",
    "global",
    "worldwide",
    "metadata",
    "enrichment"
  ],
  "author": "Vaporjawn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Vaporjawn/universities/issues"
  },
  "homepage": "https://github.com/Vaporjawn/universities#readme",
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/nock": "^10.0.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "nock": "^14.0.10",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "commander": "^14.0.1",
    "csv-parser": "^3.2.0",
    "fs-extra": "^11.3.2",
    "p-limit": "^7.1.1",
    "p-queue": "^8.1.1",
    "ts-node": "^10.9.2"
  }
}
